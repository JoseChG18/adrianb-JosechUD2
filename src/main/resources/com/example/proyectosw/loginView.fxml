<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="283.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectosw.Main">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="385.0" prefWidth="600.0">
        <children>
            <Label layoutX="22.0" layoutY="57.0" prefHeight="17.0" prefWidth="59.0" text="Username" textAlignment="CENTER" />
            <Label layoutX="22.0" layoutY="89.0" prefHeight="17.0" prefWidth="59.0" text="Password" fx:id="lblPass"/>
            <TextField fx:id="user" layoutX="89.0" layoutY="53.0" />
            <PasswordField fx:id="password" layoutX="89.0" layoutY="85.0" onKeyPressed="#pressEnter" />
            <Button fx:id="btnLogin" layoutX="104.0" layoutY="124.0" mnemonicParsing="false" onAction="#btnEvent" text="Login" />
            <Label fx:id="cabecera" layoutX="67.0" layoutY="14.0" prefHeight="33.0" prefWidth="149.0" text="INICIO SESIÓN" textAlignment="CENTER">
                <font>
                   <Font size="20.0" />
                </font>
            </Label>
            <Button fx:id="nuevoUser" layoutX="60.0" layoutY="160.0" onAction="#newUser" prefHeight="20.0" prefWidth="140.0" text="¿No tienes una cuenta?" textAlignment="CENTER" />
            <Button fx:id="cambioPass" layoutX="44.0" layoutY="188.0" onAction="#changePass" prefHeight="17.0" prefWidth="172.0" text="¿Has olvidado tu contraseña?" textAlignment="CENTER" />
        </children>
    </AnchorPane>
</VBox>
